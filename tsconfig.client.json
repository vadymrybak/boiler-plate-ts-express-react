        "module": "ESNext",
        "esModuleInterop": true,
        "target": "ES5",
        "noImplicitAny": true,
        "moduleResolution": "Node",
        "sourceMap": true,
        "outDir": "build",
        "baseUrl": ".",
        "jsx": "react-jsx",
        "paths": {
            "*": [
                "node_modules/*"
            ]
        }
    },
    "include": [
        "./"
    ]
}